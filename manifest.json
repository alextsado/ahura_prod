{
  "name": "Ahura AI Study Sessions",
  "short_name": "Study Sessions",
  "version": "1.0.0",
  "author": "Ahura AI",
  "homepage_url": "https://www.ahura.ai/",
  "description": "Focus and measure your study sessions.",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Ahura AI",
    "default_icon": "/images/favicon.png"
  },
  "permissions": ["tabs", "storage", "http://3.132.191.11/*", "https://www.ahura.ai/*"],
  "background": {
    "page": "html/background.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "run_at": "document_idle",
      "js": ["src/ext/content.js"]
    }
  ],
  "key": "",
  "icons": {
    "128": "images/favicon.png"
  },
  "externally_connectable": {
    "matches": ["https://www.ahura.ai/*"]
  },
  "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com/ajax/libs/p5.js/0.9.0/addons/p5.dom.min.js https://cdnjs.cloudflare.com/ajax/libs/p5.js/0.9.0/p5.min.js https://unpkg.com/@tensorflow-models/face-landmarks-detection@0.0.1/dist/face-landmarks-detection.js https://unpkg.com/@tensorflow/tfjs-backend-webgl@2.4.0/dist/tf-backend-webgl.js https://unpkg.com/@tensorflow/tfjs-backend-cpu@2.4.0/dist/tf-backend-cpu.js https://unpkg.com/@tensorflow/tfjs-core@2.4.0/dist/tf-core.js https://unpkg.com/@tensorflow/tfjs-converter@2.4.0/dist/tf-converter.js https://unpkg.com/@tensorflow/tfjs-core@1.7.0/dist/tf-core.js 'unsafe-eval'; object-src 'self'"
}
